{
  "name": "postgresql",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "rimraf ../public/{js,css,img} && vue-cli-service build --no-clean",
    "dev": "rimraf ../public/{js,css,img} && vue-cli-service build --mode development --no-clean --watch",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bootstrap-vue": "^2.16.0",
    "vue": "2.6.12",
    "vue-router": "3.4.3",
    "vue2-transitions": "0.3.0",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-undef": "off",
      "vue/no-unused-components": "off",
      "vue/no-dupe-keys": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
